o.id)\n        const style = {\n            border: '1px solid black',\n            padding: '8px',\n            margin: '8px'\n        }\n\n        const {editing} = this.state\n\n        if(editing){\n            return(\n                <div style={style}>\n                    <div>\n                        <input\n                            value={this.state.name}\n                            name=\"name\"\n                            placeholder=\"이름\"\n                            onChange={this.handleChange}\n                        />\n                    </div>\n                    <div>\n                        <input \n                            value={this.state.phone}\n                            name=\"phone\"\n                            placeholder=\"전화번호\"\n                            onChange={this.handleChange}\n                        />\n                    </div>\n                    <button onClick={this.handleToggleEdit}>적용</button>\n                    <button onClick={this.handleRemove}>삭제</button>\n                </div>\n            )\n        }\n        \n        const {\n            name, phone\n        } = this.props.info\n\n        return(\n            <div style={style}>\n                <div><b>{name}</b></div>\n                <div>{phone}</div>\n                <button onClick={this.handleToggleEdit}>수정</button>\n                <button onClick={this.handleRemove}>삭제</button>\n            </div>\n        )\n    }    \n}\nexport default PhoneInfo","/Users/lora/react_study/hello-react/src/component/PhoneInfoList.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/AtomFamily.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/Animals.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/images.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/Metadata.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/store.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/image.js",[],"/Users/lora/react_