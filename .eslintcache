c/component/PhoneForm.js",[],"/Users/lora/react_study/hello-react/src/component/PhoneInfo.js",["92"],"import React, {Component} from 'react'\n\nclass PhoneInfo extends Component {\n    static defaultProps = {\n        info:{\n            name: '이름',\n            phone: '010-0000-0000',\n            id: 0\n        }\n    }\n\n    state = {\n        editing: false,\n        name: '',\n        phone: ''\n    }\n\n    handleRemove = ()=>{\n        const {info, onRemove} = this.props\n        onRemove(info.id)\n    }\n\n    handleToggleEdit = () => {\n        const {editing} = this.state\n        this.setState({ editing: !editing})\n    }\n\n    handleChange = (e) =>{\n        const {name, value} = e.target\n        this.setState({\n            [name]: value\n        })\n    }\n\n    shouldComponentUpdate(nextProps, nextState){\n        if(!this.state.editing\n            && !nextState.editing\n            && nextProps.info === this.props.info){\n                return false\n        }\n        return true\n    }\n\n    componentDidUpdate(prevProps, prevState){\n        const {info, onUpdate} = this.props\n        if(!prevState.editing && this.state.editing){\n            this.setState({\n                name: info.name,\n                phone: info.phone\n            })\n        }\n    }\n\n    render() {\n        console.log('render PhoneInfo '+this.props.info.id)\n        const style = {\n            border: '1px solid black',\n            padding: '8px',\n            margin: '8px'\n        }\n\n        const {editing} = this.state\n\n        if(editing){\n            return(\n                <div style={style}>\n                    <div>\n                        <input\n                            value={this.state.name}\n                            name=\"name\"\n                            placeholder=\"이름\"\n                            onChange={this.handleChange}\n                        />\n                    </div>\n                    <div>\n                        <input \n                            value={this.state.phone}\n                            name=\"phone\"\n                            placeholder=\"전화번호\"\n                            onChange={this.handleChange}\n                        />\n                    </div>\n                    <button onClick={this.handleToggleEdit}>적용</button>\n                    <button onClick={this.handleRemove}>삭제</button>\n                </div>\n            )\n        }\n        \n        const {\n            name, phone\n        } = this.props.info\n\n        return(\n            <div style={style}>\n                <div><b>{name}</b></div>\n                <div>{phone}</div>\n                <button onClick={this.handleToggleEdit}>수정</button>\n                <button onClick={this.handleRemove}>삭제</button>\n            </div>\n        )\n    }    \n}\nexport default PhoneInfo","/Users/lora/react_study/hello-react/src/component/PhoneInfoList.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/AtomFamily.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/Animals.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/images.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/Metadata.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/store.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/image.js",[],"/Users/lora/react_study/hello-react/src/recoilTest/Selector.js",[],"/Users/lora/react_study/hello-react/src/ReactGridLayout/ShowcaseLayout.js",[],"/Users/lora/react_study/hello-react/src/ReactGridLayout/index.js",[],"/Users/lora/react_study/hello-react/src/chart/App.js",[],"/Users/lora/react_study/hello-react/src/chart/LineChart.js",[],["93","94"],"/Users/lora/react_study/hello-react/src/LayoutInChart/ShowcaseLayout.js",[],"/Users/lora/react_study/hello-react/src/LayoutInChart/LineChart.js",[],"/Users/lora/react_study/hello-react/src/LayoutInChart/App.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleI